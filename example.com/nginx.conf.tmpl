rewrite ^{{mount_point}}$ {{mount_point}}special/home.html last;

location {{mount_point}}static/ {
    root {{server_name}};
}

location {{mount_point}}special/ {
    internal;
    root {{server_name}};
}

location {{mount_point}} {
    include uwsgi_params;
    uwsgi_pass unix:{{server_name}}/uwsgi.sock;
}
